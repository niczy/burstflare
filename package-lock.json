{
  "name": "burstflare",
  "version": "0.1.0",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "burstflare",
      "version": "0.1.0",
      "workspaces": [
        "apps/*",
        "packages/*"
      ],
      "engines": {
        "node": ">=20.0.0"
      }
    },
    "apps/cli": {
      "name": "@burstflare/cli",
      "bin": {
        "burstflare": "bin/burstflare.js"
      }
    },
    "apps/edge": {
      "name": "@burstflare/edge"
    },
    "apps/web": {
      "name": "@burstflare/web"
    },
    "node_modules/@burstflare/cli": {
      "resolved": "apps/cli",
      "link": true
    },
    "node_modules/@burstflare/edge": {
      "resolved": "apps/edge",
      "link": true
    },
    "node_modules/@burstflare/shared": {
      "resolved": "packages/shared",
      "link": true
    },
    "node_modules/@burstflare/web": {
      "resolved": "apps/web",
      "link": true
    },
    "packages/shared": {
      "name": "@burstflare/shared"
    }
  }
}
