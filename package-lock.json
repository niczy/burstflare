{
  "name": "burstflare",
  "version": "0.1.0",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "burstflare",
      "version": "0.1.0",
      "workspaces": [
        "apps/*",
        "packages/*"
      ],
      "dependencies": {
        "@cloudflare/containers": "^0.1.1"
      },
      "engines": {
        "node": ">=20.0.0"
      }
    },
    "apps/cli": {
      "name": "@burstflare/flare",
      "version": "0.1.0",
      "license": "UNLICENSED",
      "bin": {
        "flare": "bin/flare.js"
      },
      "engines": {
        "node": ">=20.0.0"
      }
    },
    "apps/edge": {
      "name": "@burstflare/edge"
    },
    "apps/web": {
      "name": "@burstflare/web"
    },
    "node_modules/@burstflare/edge": {
      "resolved": "apps/edge",
      "link": true
    },
    "node_modules/@burstflare/flare": {
      "resolved": "apps/cli",
      "link": true
    },
    "node_modules/@burstflare/shared": {
      "resolved": "packages/shared",
      "link": true
    },
    "node_modules/@burstflare/web": {
      "resolved": "apps/web",
      "link": true
    },
    "node_modules/@cloudflare/containers": {
      "version": "0.1.1",
      "resolved": "https://registry.npmjs.org/@cloudflare/containers/-/containers-0.1.1.tgz",
      "integrity": "sha512-YTdobRTnTlUOUPMFemufH367A9Z8pDfZ+UboYMLbGpO0VlvEXZDiioSmXPQMHld2vRtkL31mcRii3bcbQU6fdw==",
      "license": "ISC"
    },
    "packages/shared": {
      "name": "@burstflare/shared"
    }
  }
}
